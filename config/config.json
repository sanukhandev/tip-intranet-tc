{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "welcome-message-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/welcomeMessage/WelcomeMessageWebPart.js",
          "manifest": "./src/webparts/welcomeMessage/WelcomeMessageWebPart.manifest.json"
        }
      ]
    },
    "banner-carousel-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/bannerCarousel/BannerCarouselWebPart.js",
          "manifest": "./src/webparts/bannerCarousel/BannerCarouselWebPart.manifest.json"
        }
      ]
    },
    "quick-links-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/quickLinks/QuickLinksWebPart.js",
          "manifest": "./src/webparts/quickLinks/QuickLinksWebPart.manifest.json"
        }
      ]
    },
    "buy-sell-items-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/buySellItems/BuySellItemsWebPart.js",
          "manifest": "./src/webparts/buySellItems/BuySellItemsWebPart.manifest.json"
        }
      ]
    },
    "company-calender-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/companyCalender/CompanyCalenderWebPart.js",
          "manifest": "./src/webparts/companyCalender/CompanyCalenderWebPart.manifest.json"
        }
      ]
    },
    "tabbed-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/tabbedList/TabbedListWebPart.js",
          "manifest": "./src/webparts/tabbedList/TabbedListWebPart.manifest.json"
        }
      ]
    },
    "snap-share-and-photo-gallery-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/snapShareAndPhotoGallery/SnapShareAndPhotoGalleryWebPart.js",
          "manifest": "./src/webparts/snapShareAndPhotoGallery/SnapShareAndPhotoGalleryWebPart.manifest.json"
        }
      ]
    },
    "discussion-board-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/discussionBoard/DiscussionBoardWebPart.js",
          "manifest": "./src/webparts/discussionBoard/DiscussionBoardWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "WelcomeMessageWebPartStrings": "lib/webparts/welcomeMessage/loc/{locale}.js",
    "BannerCarouselWebPartStrings": "lib/webparts/bannerCarousel/loc/{locale}.js",
    "QuickLinksWebPartStrings": "lib/webparts/quickLinks/loc/{locale}.js",
    "BuySellItemsWebPartStrings": "lib/webparts/buySellItems/loc/{locale}.js",
    "CompanyCalenderWebPartStrings": "lib/webparts/companyCalender/loc/{locale}.js",
    "TabbedListWebPartStrings": "lib/webparts/tabbedList/loc/{locale}.js",
    "SnapShareAndPhotoGalleryWebPartStrings": "lib/webparts/snapShareAndPhotoGallery/loc/{locale}.js",
    "DiscussionBoardWebPartStrings": "lib/webparts/discussionBoard/loc/{locale}.js"
  }
}
